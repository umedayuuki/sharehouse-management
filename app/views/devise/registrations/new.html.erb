<div class="card col-sm-6 mx-auto mt-5 mb-5">
  <div class="card-body mx-auto">
    <h4 class="mt-1 mb-3">新規登録</h4><br>

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <%= render "devise/shared/error_messages", resource: resource %>
      <div class="form-group">
        <%= f.label "名前（シェアハウス）" %><br>
        <%= f.text_field :house_name, autofocus: true, autocomplete: "house_name" %>
      </div>

      <div class="form-group">
        <%= f.label "住所（シェアハウス）" %><br>
        <%= f.text_field :address, autofocus: true, autocomplete: "address" %>
      </div>

      <div class="form-group">
        <%= f.label "パスワード（6文字以上）" %><br>
        <%= f.password_field :password, autocomplete: "new-password" %>
      </div>

      <div class="form-group">
        <%= f.label "パスワード（確認用）" %><br>
        <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
      </div>

      <div class="actions">
        <%= f.submit "登録", :class => "btn btn-sm btn-outline-dark disabled" %>
      </div>

    <% end %>

  </div>
</div>

<%#= render "devise/shared/links" %>
