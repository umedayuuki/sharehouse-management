<div class="contents row">
  <div class="container">
    <h3>ユーザー登録</h3>
    <%= form_with(model: @user, local: true) do |form| %>
    <%= render 'layouts/error_messages', model: form.object %>
      <%= form.file_field :image, id:"item-image" %><br>
      <%= form.text_field :user_name, placeholder: "User_name" %><br>
      <%= form.text_field :birthday, placeholder: "Birthday" %><br>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
<div>
  <%= link_to "住民一覧へ戻る",  users_path, class: "post" %>
</div>
<div>
  <%= link_to "メニュー画面へ戻る",  root_path, class: "post" %>
</div>