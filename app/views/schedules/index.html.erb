<div>
<%= link_to "メニュー画面へ戻る", root_path, class: "post" %>
</div>
<div>
  <%= link_to "スケシュールの登録",  new_schedule_path, class: "post" %>
</div>
<br>
<% @schedules.each do |schedule| %>
  <% if current_house.id == schedule.house_id %>
    投稿者<br>
    <%= schedule.user_name %><br>
    TODO<br>
    <%= schedule.day %><br>
    期限<br>
    <%= schedule.event %><br>
    <br>
    <%= link_to schedule_path(schedule.id) do %>
      編集・削除<br>
    <% end %>
  <% end %>
<% end %>