<div class="contents row">
  <div class="container">
    <h3>リストを編集する</h3>
    <%= form_with(model: @schedule, local: true) do |form| %>
    <%= render 'layouts/error_messages', model: form.object %>
      <%= form.select :user_name, @users.map{ |user| user.user_name }, include_blank: "住民を選択" %><br>
      <%= form.text_field :day, placeholder: "day" %><br>
      <%= form.text_field :event, placeholder: "event" %><br>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>