<div>
<%= link_to "メニュー画面へ戻る", root_path, class: "post" %>
</div>
<div>
  <%= link_to "リストの登録",  new_todo_path, class: "post" %>
</div>
<br>
<% @todos.each do |todo| %>
  <% if current_house.id == todo.house_id %>
    投稿者<br>
    <%= todo.user_name %><br>
    TODO<br>
    <%= todo.list %><br>
    期限<br>
    <%= todo.limit %><br>
    <br>
    <%= link_to todo_path(todo.id) do %>
      編集・削除<br>
    <% end %>
  <% end %>
<% end %>